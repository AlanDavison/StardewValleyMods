{
    "$schema": "https://smapi.io/schemas/content-patcher.json",
    "Changes": [
        {
            "Action": "Include",
            "FromFile": "Data/NPCs/ModderBob/Dialogue.json"
        },
        {
            "Action": "Include",
            "FromFile": "Data/NPCs/ModderBob/Disposition.json"
        },
        {
            "Action": "Include",
            "FromFile": "Data/NPCs/ModderBob/Events.json"
        },
        {
            "Action": "Include",
            "FromFile": "Data/NPCs/ModderBob/GiftTastes.json"
        },
        {
            "LogName": "SVE not present. Loading vanilla map patches.",
            "Action": "Include",
            "FromFile": "Data/NPCs/ModderBob/Schedule.json",
            "When": {
                "HasMod |contains={{SVE_Mod_ID}}": false
            }
        },
        {
            "LogName": "SVE was present. Loading SVE map patches.",
            "Action": "Include",
            "FromFile": "Data/NPCs/ModderBob/Schedule_SVE.json",
            "When": {
                "HasMod |contains={{SVE_Mod_ID}}": true
            }
        }
    ]
}
